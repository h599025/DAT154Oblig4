<Window x:Class="CleaningSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CleaningSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Cleaning" HorizontalAlignment="Center" FontSize="25" Margin="0,101,0,0" VerticalAlignment="Top" Height="70" Width="314" Click="Cleaning"/>
        <Button Content="Service" HorizontalAlignment="Center" FontSize="25" VerticalAlignment="Top" Height="65" Width="314" Click="Service" Margin="0,199,0,0"/>
        <Button Content="Maintenance" HorizontalAlignment="Center" FontSize="25" Margin="0,290,0,0" VerticalAlignment="Top" Height="69" Width="314" Click="Maintenance"/>
        <Label Content="Service Role" FontSize="45" HorizontalAlignment="Left" Margin="276,14,0,0" VerticalAlignment="Top" Width="267" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.484"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.54"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>
